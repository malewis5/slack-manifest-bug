{
  "name": "slack-dev-platform",
  "version": "0.1.0",
  "description": "Create and install Slack apps programmatically",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "slack-dev": "./dist/cli/create-app.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/cli/create-app.ts",
    "create-app": "tsx src/cli/create-app.ts"
  },
  "dependencies": {
    "@ngrok/ngrok": "^1.7.0",
    "dotenv": "^16.4.7",
    "open": "^10.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.0",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0"
  }
}
